{
  "name": "docs-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./build tsconfig.tsbuildinfo",
    "start": "yarn run start-ts && yarn run lint",
    "start-ts": "react-scripts start",
    "build": "yarn run build-ts && yarn run lint",
    "build-ts": "react-scripts build",
    "lint": "eslint -c eslint.config.js",
    "serve": "yarn run build && yarn run serve-no-build -l 3000",
    "serve-no-build": "serve -s build"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "react-scripts": "5.0.1",
    "rimraf": "^5.0.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
